<script type="text/javascript">
	var num=688;//每平方多少钱
    var count=0;//总价格
    var alert_word="";
	var post_flag = false;
$("#baojia_submit").click(function(){
	var bj_mj=$(".bj_mianji").val();
	var bj_name=$(".bj_name").val();
	var bj_phone=$(".bj_phone").val();
	console.log(bj_mj+"--"+bj_name+"--"+bj_phone);
	console.log($(".bj_name").attr("placeholder"));
	var alert_num=/^[0-9]*$/;
	var alert_name=/^[\u4E00-\u9FFF]+$/;
	var alert_phone=/^0?1[3|4|5|8][0-9]\d{8}$/;

	if(bj_mj==""||bj_name==""||bj_phone==""||!alert_num.test(bj_mj)||!alert_name.test(bj_name)||!alert_phone.test(bj_phone)){
		alert('请认真填写，计算更精确哦！！！');
    	return false;

	}else{		
		var nowDate = new Date();
		var str = nowDate.getFullYear()+"-"+(nowDate.getMonth() + 1)+"-"+nowDate.getDate()+" "+nowDate.getHours()+":"+nowDate.getMinutes()+":"+nowDate.getSeconds();
		document.getElementById("times").value=str;
		
		var message_phone	=document.getElementById("message_phone");
		var message_tel    =document.getElementById("message_tel");
		var message_name	=document.getElementById("message_name");
		var message_qq    =document.getElementById("message_qq");
		var times    	=document.getElementById("times");
		var source    	=$("#source_baojia_jisuan").val();
		var dede_fieldshash    	=$("#dede_fieldshash_baojia_jisuan").val();

		if (post_flag) return ;
		post_flag = true;

		//定义变量接收上面表单每项的值和几个dede隐藏的input的值
		var dataString = 'message_phone='+ message_phone.value + '&message_tel=' + message_tel.value + '&message_name=' + message_name.value +  '&message_qq=' + message_qq.value + '&source=' +source +  '&times=' + times.value +'&action=post'+ '&diyid=2&do=2&dede_fields=message_phone,select;message_tel,text;message_name,text;message_qq,text;times,text&dede_fieldshash='+dede_fieldshash;
		$.ajax({
			type: "POST",
			url: "http://www.n8house.com/plus/diy.php", //提交到后台文件
			data: dataString, //传值
			success: function(data) {
	            post_flag = false;
				alert(data);//成功打印PHP返回的值
			
			}
		});
		count=num*parseInt(bj_mj);
		window.parent.$("#bj_em1").text(count/10000);
		window.parent.$("#bj_em2").text(count/10000);
		window.parent.$("#bj_ws").text(parseInt(count*30/100)+"元");
		window.parent.$("#bj_kt").text(parseInt(count*20/100)+"元");
		window.parent.$("#bj_cf").text(parseInt(count*20/100)+"元");
		window.parent.$("#bj_wc").text(parseInt(count*12/100)+"元");
		window.parent.$("#bj_yt").text(parseInt(count*8/100)+"元");
		window.parent.$("#bj_qt").text(parseInt(count*10/100)+"元");
	}
})
</script>

