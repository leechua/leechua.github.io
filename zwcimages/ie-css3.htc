function add_ajaxmessage(){
	var nowDate = new Date();
	var str = nowDate.getFullYear()+"-"+(nowDate.getMonth() + 1)+"-"+nowDate.getDate()+" "+nowDate.getHours()+":"+nowDate.getMinutes()+":"+nowDate.getSeconds();
	document.getElementById("times").value=str;
	

    var name=document.getElementById("namezc");
    var tel    =document.getElementById("telzc");
    var times    =document.getElementById("times");
	
	var reg = /^(?:1\d)\d{6}(\d{3}|\*{3})$/;
	var	xing=/^[\u4e00-\u9fa5 ]{2,8}$/;	
	
    
    //定义变量接收上面表单每项的值和几个dede隐藏的input的值
    var dataString = 'name='+ name.value + '&tel=' + tel.value + '&times=' + times.value +'&action=post'+ '&diyid=1&do=2&dede_fields=name,text;tel,text;times,text&dede_fieldshash=a05f92dbf6105a765fa00c8885bebb39';

	if(xing.test(name.value) && reg.test(tel.value)){
	   $.ajax({
        type: "POST",
        url: "http://www.n8house.com/plus/diy.php", //提交到后台文件
        data: dataString, //传值
        success: function(data) {

            alert(data);//成功打印PHP返回的值
        }
    });
	}else{
